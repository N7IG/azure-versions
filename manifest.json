{
    "name": "Webapps info",
    "version": "1.0",
    "description": "Current info.json version",
    "manifest_version": 2,
    "permissions": ["declarativeContent", "tabs", "<all_urls>"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png"
        }
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [{

        "matches": ["<all_urls>"],

        "js": ["contentScript.js"]

      }]
}
